<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_config_settings_view_form" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.inherit.business.card</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base_setup.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <div id="multi_company" position="after">
                <h2 groups="base.group_system">Google cloud api access</h2>
                 <div groups="base.group_system" class="row mt16 o_settings_container">
                    <div class="col-xs-12 col-md-12 o_setting_box">
                        <label for="google_api_json_file_content"/>
                        <field name="google_api_json_file_content"/>
                    </div>
                </div>
            </div>
        </field>
    </record>
</odoo> 

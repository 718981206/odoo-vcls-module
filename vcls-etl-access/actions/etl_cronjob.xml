<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="cron_Update_Access" model="ir.cron">
            <field name="name">ETL Update Keys Access</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="model_id" ref="model_etl_sync_access_keys" />
            <field name="state">code</field>
            <field name="code">full_update = True
obj_dict = {
    'do_client':True,
    'do_contact':True,
    }
model.access_update_keys(obj_dict,full_update)
            </field>
        </record>

        <record id="cron_process_access" model="ir.cron">
            <field name="name">ETL Process Keys Access</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="numbercall">-1</field>
            <field name="interval_number">10</field>
            <field name="interval_type">minutes</field>
            <field name="doall">False</field>
            <field name="model_id" ref="model_etl_sync_access" />
            <field name="state">code</field>
            <field name="code">batch_size = None
loop = False
max_minutes = 9
model.access_process_keys(batch_size,loop,max_minutes)
            </field>
        </record>

        <record id="cron_relaunch_access" model="ir.cron">
            <field name="name">ETL Relaunch Process Keys Access</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="numbercall">-1</field>
            <field name="interval_number">1</field>
            <field name="interval_type">minutes</field>
            <field name="doall">False</field>
            <field name="model_id" ref="model_etl_sync_access_keys" />
            <field name="state">code</field>
            <field name="code">model.relaunch_cron('vcls-etl-access.cron_process',2)
            </field>
        </record>
        
    </data>
</odoo>
            <field name="arch" type="xml">
                <tree create='1' editable='bottom'>
                    <field name="name"/>
                    <field name="active"/> 
                </tree>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="action_partner_seniority">
            <field name="name">Partner Seniority</field>
            <field name="res_model">partner.seniority</field>
            <field name="view_mode">tree</field>
        </record>
        

    </data>
</odoo>

                    <field name="active"/> 
                </tree>
            </field>
        </record>
        
        <record model="ir.actions.act_window" id="action_client_product">
            <field name="name">Client Product</field>
            <field name="res_model">client.product</field>
            <field name="view_mode">tree</field>
        </record>
        
        <record model="ir.ui.view" id="view_functional_focus_tree">
            <field name="name">view_functional_focus_tree</field>
            <field name="model">partner.functional.focus</field>
            <field name="arch" type="xml">

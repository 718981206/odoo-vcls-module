<odoo>
    <data>
        <record id="view_project_formm_inherit" model="ir.ui.view">
            <field name="name">project.form</field>
            <field name="model">project.project</field>
            <field name="priority" eval="1"/>
            <field name="inherit_id" ref="project.edit_project"/>
            <field name="arch" type="xml">
                <div name="options_active" position="before">
                    <field name="project_type" readonly="1"/>
                </div>
            </field>
        </record>
        
        <record id="view_project_form_contact_book" model="ir.ui.view">
            <field name="name">project.form.contact.book</field>
            <field name="model">project.project</field>
            <field name="priority" eval="500"/>
            <field name="arch" type="xml">
                <form string="Project Contact Book">
                    <sheet>
                        <group>
                            <group>
                                <field name='name' string="Project Name"/>
                            </group>
                            <group>
                                <field name='alias_user_id' string="Lead Consultant"/>
                            </group>
                        </group>
                        <group>
                            <group string="Consultants">
                                <field name='consultant_ids' mode="tree">
                                    <tree>
                                        <field name="name"/>
                                        <field name="job_id"/>
                                    </tree>
                                </field>  
                            </group>
                            <group string="Technical Advisors" >
                                <field name='ta_ids' mode="tree">
                                    <tree>
                                        <field name="name"/>
                                        <field name="job_id"/>
                                    </tree>
                                </field>
                            </group>
                        </group>
                        <!--<notebook>
                            <page string="Consultants" name="consultant">
                                <field name='consultant_ids' mode="tree">
                                    <tree editable="top">
                                        <field name="name"/>
                                        <field name="job_id"/>
                                    </tree>
                                </field>
                            </page>

                            <page string="Ta" name="ta">
                                <field name='ta_ids'>
                                    <tree editable="top">
                                        <field name="name"/>
                                        <field name="job_id"/>
                                    </tree>
                                </field>
                            </page>
                        </notebook>-->
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>
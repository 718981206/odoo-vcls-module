<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cron_timesheet" model="ir.cron">
            <field name="name">Approve Timesheet</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_admin" />
            <field name="interval_number">1</field>
            <field name="interval_type">weeks</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="model_id" ref="model_hr_employee" />
            <field name="state">code</field>
            <field name="code">model.approve_timesheets(36)</field>
        </record>

        <record id="cron_group_project_manager" model="ir.cron">
            <field name="name">List Project Manager</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_admin" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="model_id" ref="model_project_project" />
            <field name="state">code</field>
            <field name="code">model.update_project_manager()</field>
        </record>
    </data>
</odoo>
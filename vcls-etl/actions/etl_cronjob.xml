        <record id="etl_relauncher" model="ir.cron">
            <field name="name">relauncher</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="numbercall">-1</field>
            <field name="doall">False</field>
            <field name="model_id" ref="base.model_ir_cron" />
            <field name="state">code</field>
            <field name="code">relauncher = model.search([('name','ilike','relauncher')],limit=1)
cronName = relauncher.name.split(' ',1)[1]
toRelaunch = model.search([('name','ilike',cronName)],limit=1)

if relauncher.active:
    toRelaunch.write({'active':True, 'nextcall': datetime.datetime.now()})
else:
    relauncher.write({'name':'relauncher'})
    toRelaunch.write({'active':False})    
            </field>
        </record>


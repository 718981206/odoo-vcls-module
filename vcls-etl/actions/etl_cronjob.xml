<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
    
        <record id="cron_etl" model="ir.cron">
            <field name="name">ETL Saleforce / Odoo</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="interval_number">1</field>
            <field name="interval_type">hours</field>
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="model_id" ref="model_etl_salesforce_account" />
            <field name="state">code</field>
            <field name="code">model.run(False)</field>
        </record>

        <record id="cron_etl_full_update" model="ir.cron">
            <field name="name">ETL Odoo full update from Salesforce</field>
            <field name="active" eval="False" />
            <field name="user_id" ref="base.user_admin" />
            <field name="numbercall">-1</field>
            <field name="doall">1</field>
            <field name="model_id" ref="model_etl_salesforce_account" />
            <field name="state">code</field>
            <field name="code">model.run(True)</field>
        </record>

    </data>
</odoo>
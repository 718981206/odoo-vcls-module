<odoo>
    <data noupdate="1">
        <record id="SF_mail" model="ir.config_parameter">
            <field name="key">sf.mail</field>
            <field name="value">mail</field>
        </record>

        <record id="SF_password" model="ir.config_parameter">
            <field name="key">sf.password</field>
            <field name="value">password</field>
        </record>

        <record id="SF_token" model="ir.config_parameter">
            <field name="key">sf.token</field>
            <field name="value">token</field>
        </record>

        <record id="ETL_ToRelaunch" model="ir.config_parameter">
            <field name="key">sf.toRelauch</field>
            <field name="value">ETL</field>
        </record>

        <record id="ETL_LastRun" model="ir.config_parameter">
            <field name="key">sf.last_run</field>
            <field name="value">2000-01-01 00:00:00.000000+00</field>
        </record>

    </data>

</odoo>
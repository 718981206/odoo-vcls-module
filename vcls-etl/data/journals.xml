<odoo>
    <data>
        <record id="bank_of_america" model="account.journal">
            <field name="name">Bank of America</field>
            <field name="type">bank</field>
            <field name="company_id" ref="vcls-hr.company_VCINC"/>
            <field name="code">BOA</field>
        </record>

        <record id="sales_journal" model="account.journal">
            <field name="name">Sales</field>
            <field name="type">sale</field>
            <field name="company_id" ref="vcls-hr.company_VCINC"/>
            <field name="code">SALES</field>
        </record>

        <record id="manual_adjustment" model="account.journal">
            <field name="name">Manual Adjustment</field>
            <field name="type">general</field>
            <field name="company_id" ref="vcls-hr.company_VCINC"/>
            <field name="code">MA</field>
        </record>

        <record id="purchase_journal" model="account.journal">
            <field name="name">Purchase</field>
            <field name="type">purchase</field>
            <field name="company_id" ref="vcls-hr.company_VCINC"/>
            <field name="code">PU</field>
        </record>

        <record id="billed_journal" model="account.journal">
            <field name="name">Billed</field>
            <field name="type">general</field>
            <field name="company_id" ref="vcls-hr.company_VCINC"/>
            <field name="code">BILL</field>
        </record>

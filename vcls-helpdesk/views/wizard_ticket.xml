<?xml version="1.0"?>

<odoo>
  <data>
        <record id="view_wizard_ticket_form_default" model="ir.ui.view">
            <field name="name">view_wizard_ticket_form_default</field>
            <field name="model">wizard.ticket</field>
            <field name="arch" type="xml">
                <form string="Reply To Support">
                    <group>
                        <field name="message"/>
                    </group>
                    
                    <footer>
                        <button string="Submit" name="set_to_progess" type="object" class="btn-primary"/>
                        <button string="Discard" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
            

        </record>
    </data>
</odoo>